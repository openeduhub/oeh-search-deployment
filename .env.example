# Root URL where the service will be accessible from the Internet.
URL=https://suche.wirlernenonline.de

# Root URL of the corresponding WLO Wordpress page.
WORDPRESS_URL=https://wirlernenonline.de

# URL of the Edu-Sharing instance for editorial work.
EDUSHARING_URL=https://redaktion.openeduhub.net/edu-sharing

# URL of the Elastic instance to use for telemetry.
ELASTICSEARCH_URL=http://some.host:9200

# Display a link to experimental-feature toggles in the frontend.
SHOW_EXPERIMENTS=false

# Ports to provide the services at on localhost.
FRONTEND_PORT=8080
ANALYTICS_PORT=3001

# Release branch to decide which docker images to use for frontend and analytics. Possible values
# are 'develop', 'master', and tags of the form 'v1.2.3'.
FRONTEND_RELEASE=master
ANALYTICS_RELEASE=master
